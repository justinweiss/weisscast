<h1>New audio_file</h1>

<%= error_messages_for :audio_file %>

<% form_for(@audio_file, :url => audio_files_path, :html => {:multipart => true}) do |f| %>
  <p>
    <label for="audio_file">Upload an MP3:</label>
    <%= f.file_field :uploaded_data %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', audio_files_path %>
